<!--{**-->
<!--This code adds the new page to the MenuBar control in ShellPage -->
<!--**}-->

<Page
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <SplitView
        OpenPaneLength="400">
        <Grid>
            <winui:MenuBar VerticalAlignment="Top">
                <winui:MenuBarItem x:Uid="ShellMenuBarItem_File">
                    <!--^^-->
                    <!--{[{-->
                    <MenuFlyoutItem x:Uid="ShellMenuItem_File_wts.ItemName" Command="{x:Bind ViewModel.MenuFilewts.ItemNameCommand}" />
                    <!--}]}-->
                    <MenuFlyoutItem x:Uid="ShellMenuItem_File_Exit" Command="{x:Bind ViewModel.MenuFileExitCommand}" />
                </winui:MenuBarItem>
            </winui:MenuBar>
        </Grid>
    </SplitView>
</Page>
